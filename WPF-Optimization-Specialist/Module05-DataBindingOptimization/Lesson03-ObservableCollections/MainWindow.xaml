<Window x:Class="ObservableCollectionsDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:ObservableCollectionsDemo"
        Title="Lesson 3: ObservableCollection Optimization" Height="750" Width="1400"
        Background="#F5F5F5">
    <Window.Resources>
        <Style x:Key="HeaderStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="16"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="Foreground" Value="#2196F3"/>
            <Setter Property="Margin" Value="5,10,5,5"/>
        </Style>
        <Style x:Key="MetricStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="12"/>
            <Setter Property="FontWeight" Value="SemiBold"/>
            <Setter Property="Margin" Value="5,2"/>
        </Style>
    </Window.Resources>

    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <!-- Header -->
        <Border Grid.Row="0" Background="#2196F3" CornerRadius="5" Padding="15" Margin="0,0,0,10">
            <StackPanel>
                <TextBlock Text="ObservableCollection Optimization"
                          FontSize="24" FontWeight="Bold" Foreground="White"/>
                <TextBlock Text="Master bulk updates, range operations, and efficient collection change notifications"
                          FontSize="14" Foreground="White" Opacity="0.9" Margin="0,5,0,0"/>
            </StackPanel>
        </Border>

        <!-- Main Content -->
        <TabControl Grid.Row="1" Margin="0,0,0,10">
            <!-- Tab 1: Individual vs Bulk Operations -->
            <TabItem Header="Individual vs Bulk">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <!-- Individual Operations -->
                    <GroupBox Grid.Column="0" Header="Individual Operations (Slow)">
                        <StackPanel>
                            <TextBlock Style="{StaticResource HeaderStyle}" Text="Multiple CollectionChanged Events"/>

                            <Border Background="White" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Standard ObservableCollection:" FontWeight="Bold"/>
                                    <TextBlock TextWrapping="Wrap" FontSize="11" Margin="5">
                                        Each Add/Remove raises a separate CollectionChanged event
                                    </TextBlock>
                                    <StackPanel Orientation="Horizontal" Margin="5">
                                        <TextBlock Text="Item Count:" Width="80"/>
                                        <TextBox x:Name="IndividualCountTextBox" Text="1000" Width="100"/>
                                    </StackPanel>
                                    <Button Content="Add Items Individually" Click="AddIndividually_Click" Margin="5"/>
                                    <Button Content="Remove Items Individually" Click="RemoveIndividually_Click" Margin="5"/>
                                </StackPanel>
                            </Border>

                            <Border Background="#FFEBEE" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Performance Metrics:" FontWeight="Bold"/>
                                    <TextBlock Text="{Binding IndividualAddTime}" Style="{StaticResource MetricStyle}"/>
                                    <TextBlock Text="{Binding IndividualRemoveTime}" Style="{StaticResource MetricStyle}"/>
                                    <TextBlock Text="{Binding IndividualEventCount, StringFormat='Events Raised: {0}'}"
                                             Style="{StaticResource MetricStyle}" Foreground="Red"/>
                                    <TextBlock Text="{Binding IndividualUIUpdates, StringFormat='UI Updates: {0}'}"
                                             Style="{StaticResource MetricStyle}" Foreground="Red"/>
                                </StackPanel>
                            </Border>

                            <Border Background="White" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Problems:" FontWeight="Bold"/>
                                    <TextBlock TextWrapping="Wrap" FontSize="11">
                                        ‚ùå One event per operation (1000 adds = 1000 events)
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11">
                                        ‚ùå UI refreshes after each change
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11">
                                        ‚ùå Very slow for large collections
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11">
                                        ‚ùå Can cause UI freezing
                                    </TextBlock>
                                </StackPanel>
                            </Border>

                            <TextBlock Text="{Binding StandardCollection.Count, StringFormat='Collection Count: {0}'}"
                                     FontWeight="Bold" Margin="5"/>
                        </StackPanel>
                    </GroupBox>

                    <!-- Bulk Operations -->
                    <GroupBox Grid.Column="1" Header="Bulk Operations (Fast)">
                        <StackPanel>
                            <TextBlock Style="{StaticResource HeaderStyle}" Text="Single CollectionChanged Event"/>

                            <Border Background="White" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="RangeObservableCollection:" FontWeight="Bold"/>
                                    <TextBlock TextWrapping="Wrap" FontSize="11" Margin="5">
                                        AddRange/RemoveRange raises single event for multiple items
                                    </TextBlock>
                                    <StackPanel Orientation="Horizontal" Margin="5">
                                        <TextBlock Text="Item Count:" Width="80"/>
                                        <TextBox x:Name="BulkCountTextBox" Text="1000" Width="100"/>
                                    </StackPanel>
                                    <Button Content="Add Items in Bulk" Click="AddInBulk_Click" Margin="5"/>
                                    <Button Content="Remove Items in Bulk" Click="RemoveInBulk_Click" Margin="5"/>
                                </StackPanel>
                            </Border>

                            <Border Background="#E8F5E9" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Performance Metrics:" FontWeight="Bold"/>
                                    <TextBlock Text="{Binding BulkAddTime}" Style="{StaticResource MetricStyle}"/>
                                    <TextBlock Text="{Binding BulkRemoveTime}" Style="{StaticResource MetricStyle}"/>
                                    <TextBlock Text="{Binding BulkEventCount, StringFormat='Events Raised: {0}'}"
                                             Style="{StaticResource MetricStyle}" Foreground="Green"/>
                                    <TextBlock Text="{Binding BulkUIUpdates, StringFormat='UI Updates: {0}'}"
                                             Style="{StaticResource MetricStyle}" Foreground="Green"/>
                                </StackPanel>
                            </Border>

                            <Border Background="White" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Benefits:" FontWeight="Bold"/>
                                    <TextBlock TextWrapping="Wrap" FontSize="11">
                                        ‚úÖ One event for multiple operations
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11">
                                        ‚úÖ UI refreshes only once
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11">
                                        ‚úÖ 10-100x faster for bulk operations
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11">
                                        ‚úÖ No UI freezing
                                    </TextBlock>
                                </StackPanel>
                            </Border>

                            <TextBlock Text="{Binding RangeCollection.Count, StringFormat='Collection Count: {0}'}"
                                     FontWeight="Bold" Margin="5"/>
                        </StackPanel>
                    </GroupBox>
                </Grid>
            </TabItem>

            <!-- Tab 2: Range Operations -->
            <TabItem Header="Range Operations">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <GroupBox Grid.Row="0" Header="RangeObservableCollection Implementation">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Border Grid.Column="0" Background="White" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Available Operations:" FontWeight="Bold"/>
                                    <StackPanel Margin="10,5">
                                        <TextBlock Text="‚Ä¢ AddRange(IEnumerable&lt;T&gt;)" FontFamily="Consolas" FontSize="11"/>
                                        <TextBlock Text="‚Ä¢ RemoveRange(IEnumerable&lt;T&gt;)" FontFamily="Consolas" FontSize="11"/>
                                        <TextBlock Text="‚Ä¢ ReplaceRange(IEnumerable&lt;T&gt;)" FontFamily="Consolas" FontSize="11"/>
                                        <TextBlock Text="‚Ä¢ Reset(IEnumerable&lt;T&gt;)" FontFamily="Consolas" FontSize="11"/>
                                    </StackPanel>

                                    <Button Content="Add 500 Items" Click="DemoAddRange_Click" Margin="5,10,5,5"/>
                                    <Button Content="Remove First 250 Items" Click="DemoRemoveRange_Click" Margin="5"/>
                                    <Button Content="Replace All Items" Click="DemoReplaceRange_Click" Margin="5"/>
                                    <Button Content="Reset Collection" Click="DemoReset_Click" Margin="5"/>
                                </StackPanel>
                            </Border>

                            <Border Grid.Column="1" Background="#FFF3E0" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Operation Results:" FontWeight="Bold"/>
                                    <TextBlock Text="{Binding RangeOperationResult}" TextWrapping="Wrap"
                                             Style="{StaticResource MetricStyle}"/>
                                    <TextBlock Text="{Binding RangeOperationTime}" Style="{StaticResource MetricStyle}"/>
                                    <TextBlock Text="{Binding RangeOperationEvents}" Style="{StaticResource MetricStyle}"/>

                                    <Border Background="White" CornerRadius="3" Padding="5" Margin="5,10,5,5">
                                        <TextBlock TextWrapping="Wrap" FontSize="11">
                                            <Run FontWeight="Bold">Performance Tip:</Run>
                                            Always use range operations when adding/removing multiple items to minimize UI updates.
                                        </TextBlock>
                                    </Border>
                                </StackPanel>
                            </Border>
                        </Grid>
                    </GroupBox>

                    <GroupBox Grid.Row="1" Header="Live Collection View">
                        <ScrollViewer VerticalScrollBarVisibility="Auto">
                            <ItemsControl ItemsSource="{Binding DemoCollection}">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <Border Background="White" BorderBrush="#E0E0E0" BorderThickness="1"
                                              Margin="2" Padding="8,4">
                                            <StackPanel Orientation="Horizontal">
                                                <TextBlock Text="{Binding Id, StringFormat='#{0:D4}'}"
                                                         Width="60" FontFamily="Consolas" Foreground="Gray"/>
                                                <TextBlock Text="{Binding Name}" Width="150" Margin="10,0"/>
                                                <TextBlock Text="{Binding Value}" Foreground="Blue" Margin="10,0"/>
                                                <TextBlock Text="{Binding Timestamp, StringFormat='({0:HH:mm:ss})'}"
                                                         FontSize="10" Foreground="Gray" Margin="10,0"/>
                                            </StackPanel>
                                        </Border>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                        </ScrollViewer>
                    </GroupBox>
                </Grid>
            </TabItem>

            <!-- Tab 3: Performance Comparison -->
            <TabItem Header="Performance Comparison">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <GroupBox Grid.Column="0" Header="Benchmark Tests">
                        <StackPanel>
                            <TextBlock Style="{StaticResource HeaderStyle}" Text="Comprehensive Performance Tests"/>

                            <Border Background="White" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Test Configuration:" FontWeight="Bold"/>
                                    <StackPanel Orientation="Horizontal" Margin="5">
                                        <TextBlock Text="Items to Add:" Width="100"/>
                                        <TextBox x:Name="BenchmarkCountTextBox" Text="5000" Width="100"/>
                                    </StackPanel>
                                    <Button Content="Run Full Benchmark" Click="RunBenchmark_Click"
                                          Background="#2196F3" Foreground="White" Margin="5,10,5,5"/>
                                </StackPanel>
                            </Border>

                            <Border Background="#E3F2FD" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Test Scenarios:" FontWeight="Bold"/>
                                    <TextBlock Text="1. Individual Add Operations" FontSize="11" Margin="5,3"/>
                                    <TextBlock Text="2. Bulk Add Operations" FontSize="11" Margin="5,3"/>
                                    <TextBlock Text="3. Individual Remove Operations" FontSize="11" Margin="5,3"/>
                                    <TextBlock Text="4. Bulk Remove Operations" FontSize="11" Margin="5,3"/>
                                    <TextBlock Text="5. Mixed Operations" FontSize="11" Margin="5,3"/>
                                </StackPanel>
                            </Border>

                            <Border Background="White" CornerRadius="5" Padding="10" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="Expected Results:" FontWeight="Bold"/>
                                    <TextBlock TextWrapping="Wrap" FontSize="11" Margin="5">
                                        For 5000 items:
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11" Margin="5">
                                        ‚Ä¢ Individual: ~500-1000ms, 5000 events
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11" Margin="5">
                                        ‚Ä¢ Bulk: ~50-100ms, 1 event
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" FontSize="11" Margin="5" Foreground="Green" FontWeight="Bold">
                                        ‚Üí 10-20x performance improvement!
                                    </TextBlock>
                                </StackPanel>
                            </Border>
                        </StackPanel>
                    </GroupBox>

                    <GroupBox Grid.Column="1" Header="Benchmark Results">
                        <ScrollViewer VerticalScrollBarVisibility="Auto">
                            <StackPanel>
                                <Border Background="#FFF3E0" CornerRadius="5" Padding="10" Margin="5">
                                    <StackPanel>
                                        <TextBlock Text="Performance Results:" FontWeight="Bold" FontSize="14"/>
                                        <TextBlock Text="{Binding BenchmarkResults}"
                                                 FontFamily="Consolas" FontSize="11" TextWrapping="Wrap"
                                                 Margin="5,10,5,5"/>
                                    </StackPanel>
                                </Border>

                                <Border Background="White" CornerRadius="5" Padding="10" Margin="5">
                                    <StackPanel>
                                        <TextBlock Text="Performance Chart:" FontWeight="Bold"/>
                                        <Border Background="#F5F5F5" Height="200" Margin="5" CornerRadius="3">
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="*"/>
                                                    <RowDefinition Height="*"/>
                                                </Grid.RowDefinitions>
                                                <Border Grid.Row="0" Margin="5">
                                                    <StackPanel>
                                                        <TextBlock Text="Individual Operations" FontSize="11" FontWeight="Bold"/>
                                                        <ProgressBar Value="{Binding IndividualScore}" Maximum="100"
                                                                   Height="30" Foreground="Red" Margin="5"/>
                                                        <TextBlock Text="{Binding IndividualScoreText}" FontSize="10" Margin="5,0"/>
                                                    </StackPanel>
                                                </Border>
                                                <Border Grid.Row="1" Margin="5">
                                                    <StackPanel>
                                                        <TextBlock Text="Bulk Operations" FontSize="11" FontWeight="Bold"/>
                                                        <ProgressBar Value="{Binding BulkScore}" Maximum="100"
                                                                   Height="30" Foreground="Green" Margin="5"/>
                                                        <TextBlock Text="{Binding BulkScoreText}" FontSize="10" Margin="5,0"/>
                                                    </StackPanel>
                                                </Border>
                                            </Grid>
                                        </Border>
                                    </StackPanel>
                                </Border>

                                <Border Background="#E8F5E9" CornerRadius="5" Padding="10" Margin="5">
                                    <StackPanel>
                                        <TextBlock Text="Summary:" FontWeight="Bold"/>
                                        <TextBlock Text="{Binding BenchmarkSummary}"
                                                 TextWrapping="Wrap" FontSize="12" Margin="5"/>
                                    </StackPanel>
                                </Border>
                            </StackPanel>
                        </ScrollViewer>
                    </GroupBox>
                </Grid>
            </TabItem>

            <!-- Tab 4: Best Practices -->
            <TabItem Header="Best Practices">
                <ScrollViewer VerticalScrollBarVisibility="Auto">
                    <StackPanel Margin="10">
                        <GroupBox Header="When to Use Bulk Operations">
                            <Border Background="White" CornerRadius="5" Padding="15" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="‚úÖ Use AddRange/RemoveRange when:" FontWeight="Bold" FontSize="14" Margin="0,0,0,10"/>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        ‚Ä¢ Loading initial data from database or API
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        ‚Ä¢ Applying filter results (showing/hiding multiple items)
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        ‚Ä¢ Sorting collections (remove all, add sorted)
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        ‚Ä¢ Refreshing data (clear and reload)
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        ‚Ä¢ Batch delete operations
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        ‚Ä¢ Processing multiple selections
                                    </TextBlock>
                                </StackPanel>
                            </Border>
                        </GroupBox>

                        <GroupBox Header="Implementation Patterns">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Column="0" Background="White" CornerRadius="5" Padding="10" Margin="5">
                                    <StackPanel>
                                        <TextBlock Text="‚ùå Don't Do This:" FontWeight="Bold" Foreground="Red" Margin="0,0,0,5"/>
                                        <TextBlock FontFamily="Consolas" FontSize="11" TextWrapping="Wrap" Margin="5">
// Loading 1000 items
foreach (var item in items)
{
    collection.Add(item); // 1000 events!
}
                                        </TextBlock>
                                        <TextBlock TextWrapping="Wrap" FontSize="11" Foreground="Red" Margin="5,10,5,5">
                                            Problem: Fires 1000 CollectionChanged events, causing 1000 UI updates
                                        </TextBlock>
                                    </StackPanel>
                                </Border>

                                <Border Grid.Column="1" Background="White" CornerRadius="5" Padding="10" Margin="5">
                                    <StackPanel>
                                        <TextBlock Text="‚úÖ Do This Instead:" FontWeight="Bold" Foreground="Green" Margin="0,0,0,5"/>
                                        <TextBlock FontFamily="Consolas" FontSize="11" TextWrapping="Wrap" Margin="5">
// Loading 1000 items
collection.AddRange(items); // 1 event!
                                        </TextBlock>
                                        <TextBlock TextWrapping="Wrap" FontSize="11" Foreground="Green" Margin="5,10,5,5">
                                            Solution: Fires 1 CollectionChanged event, causing 1 UI update (10-100x faster!)
                                        </TextBlock>
                                    </StackPanel>
                                </Border>
                            </Grid>
                        </GroupBox>

                        <GroupBox Header="Advanced Scenarios">
                            <StackPanel>
                                <Border Background="#E3F2FD" CornerRadius="5" Padding="10" Margin="5">
                                    <StackPanel>
                                        <TextBlock Text="Filtering Collections" FontWeight="Bold" Margin="0,0,0,5"/>
                                        <TextBlock FontFamily="Consolas" FontSize="11" TextWrapping="Wrap">
var itemsToRemove = collection.Where(x => !x.IsVisible).ToList();
collection.RemoveRange(itemsToRemove);
                                        </TextBlock>
                                    </StackPanel>
                                </Border>

                                <Border Background="#E8F5E9" CornerRadius="5" Padding="10" Margin="5">
                                    <StackPanel>
                                        <TextBlock Text="Sorting Collections" FontWeight="Bold" Margin="0,0,0,5"/>
                                        <TextBlock FontFamily="Consolas" FontSize="11" TextWrapping="Wrap">
var sorted = collection.OrderBy(x => x.Name).ToList();
collection.ReplaceRange(sorted);
                                        </TextBlock>
                                    </StackPanel>
                                </Border>

                                <Border Background="#FFF3E0" CornerRadius="5" Padding="10" Margin="5">
                                    <StackPanel>
                                        <TextBlock Text="Refreshing Data" FontWeight="Bold" Margin="0,0,0,5"/>
                                        <TextBlock FontFamily="Consolas" FontSize="11" TextWrapping="Wrap">
var newData = await LoadDataAsync();
collection.Reset(newData); // Clear + AddRange in one operation
                                        </TextBlock>
                                    </StackPanel>
                                </Border>
                            </StackPanel>
                        </GroupBox>

                        <GroupBox Header="Performance Tips">
                            <Border Background="White" CornerRadius="5" Padding="15" Margin="5">
                                <StackPanel>
                                    <TextBlock Text="üìä Key Takeaways:" FontWeight="Bold" FontSize="14" Margin="0,0,0,10"/>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        1. Standard ObservableCollection: 1 event per operation
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        2. RangeObservableCollection: 1 event per batch
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        3. Typical speedup: 10-100x for bulk operations
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        4. Memory savings: Reduced event allocations
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5">
                                        5. UI responsiveness: No freezing during bulk updates
                                    </TextBlock>
                                    <TextBlock TextWrapping="Wrap" Margin="5" FontWeight="Bold" Foreground="#2196F3">
                                        6. Always use range operations for 10+ items
                                    </TextBlock>
                                </StackPanel>
                            </Border>
                        </GroupBox>
                    </StackPanel>
                </ScrollViewer>
            </TabItem>
        </TabControl>

        <!-- Status Bar -->
        <Border Grid.Row="2" Background="#424242" CornerRadius="5" Padding="10">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Column="0" Text="{Binding StatusMessage}"
                          Foreground="White" VerticalAlignment="Center"/>
                <TextBlock Grid.Column="1" Text="{Binding CurrentMemory, StringFormat='Memory: {0:N2} MB'}"
                          Foreground="#4CAF50" VerticalAlignment="Center" Margin="10,0,0,0"/>
            </Grid>
        </Border>
    </Grid>
</Window>
